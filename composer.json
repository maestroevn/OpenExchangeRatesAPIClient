{
    "name": "tigranmaestro/OpenExchangeRatesAPIClient",
    "description": "PHP library to consume OpenExchangeRates API",
    "keywords": [
        "api",
        "wrapper",
        "exchange",
        "rates"
    ],
    "homepage": "https://github.com/tigranmaestro/OpenExchangeRatesAPIClient",
    "license": "MIT",
    "authors": [
        {
            "name": "Tigran Petrosyan",
            "email": "tigran.maestro@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "guzzlehttp/guzzle" : ">=6.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "TigranMaestro\\OpenExchangeRatesPHPClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TigranMaestro\\OpenExchangeRatesPHPClient\\Test\\": "tests"
        }
    },
    "minimum-stability": "stable",
}
